{
    "name": "angular-dx",
    "displayName": "Angular DX",
    "description": "",
    "version": "1.0.0",
    "publisher": "finico",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/finico/vscode-angular-dx.git"
    },
    "bugs": {
        "url": "https://github.com/finico/vscode-angular-dx/issues",
        "email": "finico.a@gmail.com"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "keywords": [
        "angular",
        "definition",
        "peek",
        "references"
    ],
    "categories": [
        "Languages",
        "Other"
    ],
    "icon": "icon.png",
    "galleryBanner.color": "#f10025",
    "activationEvents": [
        "onLanguage:typescript"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc --watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "jest",
        "lint": "tslint -p . **/*.ts"
    },
    "devDependencies": {
        "@types/jest": "^23.3.2",
        "@types/node": "^10.10.1",
        "codacy-coverage": "^3.1.0",
        "jest": "^23.6.0",
        "ts-jest": "^23.10.0",
        "tslint": "^5.8.0",
        "typescript": "^3.0.3",
        "vsce": "^1.49.2",
        "vscode": "^1.1.6"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "modulePathIgnorePatterns": [
            "<rootDir>/out/",
            "<rootDir>/node_modules/"
        ],
        "coveragePathIgnorePatterns": [
            "helpers.ts$"
        ]
    }
}